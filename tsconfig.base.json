{
  "compileOnSave": false,
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "pretty": true,
    "module": "Node16",
    "moduleResolution": "Node16",
    "target": "ES2021",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "noEmitOnError": false,
    "experimentalDecorators": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "importHelpers": true,
    "strictNullChecks": true,
    "stripInternal": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext.asynciterable",
      "ES2021",
    ],
    "types": ["@cloudflare/workers-types", "node", "jest"],
    "baseUrl": ".",
    "outDir": "./dist/ts",
    "rootDir": "src"
  }
}
